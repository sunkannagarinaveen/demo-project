  replicas: 1
  selector:
    matchLabels:
      app: naveenmavenproject
  template:
    metadata:
      labels:
        app: naveenmavenproject 
    spec:
      containers:
        - name: naveenmavenproject 
          image: naveenapp.azurecr.io/naveenmavenproject
          ports:
          - containerPort: 80
